{
  "name": "social-likes",
  "version": "4.0.0",
  "private": true,
  "devDependencies": {
    "autoprefixer": "~6.1.0",
    "babel-cli": "~6.1.1",
    "babel-core": "~6.0.20",
    "babel-loader": "~6.0.1",
    "babel-preset-es2015": "~6.0.15",
    "cssnano": "~3.3.2",
    "eslint": "~1.9.0",
    "express": "~4.13.3",
    "html-webpack-plugin": "~1.6.2",
    "postcss-cli": "~2.3.2",
    "postcss-initial": "~1.4.0",
    "rimraf": "~2.4.3",
    "stylus": "~0.52.4",
    "webpack": "~1.12.2",
    "webpack-dev-middleware": "~1.2.0"
  },
  "scripts": {
    "build": "npm run build:lib && npm run build:umd && npm run build:umd:min && npm run skins && npm run skins:min",
    "build:lib": "babel src --out-dir lib",
    "#build:umd": "webpack --output-file dist/social-likes.js --config build/webpack.config.development.js",
    "#build:umd:min": "webpack --output-file dist/social-likes.min.js --config build/webpack.config.production.js",
    "clean": "rimraf lib dist",
    "start": "node build/webpack-dev-server.js",
    "lint": "eslint src",
    "prepublish": "npm run test && npm run clean && npm run build",
    "test": "npm run lint",
    "skins": "stylus skins --out lib && postcss --use autoprefixer --use postcss-initial -d lib lib/*.css",
    "skins:min": "stylus skins --out dist && postcss --use autoprefixer --use postcss-initial --use cssnano -d dist dist/*.css",
    "skins:watch": "stylus skins --out lib --watch"
  }
}
